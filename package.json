{
  "name": "blueprint-project-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "db:destroy:local": "rm .local_data/db.sqlite3",
    "db:migrate:local": "drizzle-kit migrate",
    "db:generate": "find ./src/db -name '.DS_Store' -delete && drizzle-kit generate",
    "db:seed:local": "DB_URL=.local_data/db.sqlite3 tsx scripts/seed-db.ts",
    "pre-dev": "mkdir -p .local_data",
    "dev": "DB_URL=.local_data/db.sqlite3 tsx watch --clear-screen=false src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "better-sqlite3": "^11.10.0",
    "drizzle-orm": "^0.43.1",
    "express": "^5.1.0",
    "express-zod-safe": "^1.5.0",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "uuid": "^11.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.17",
    "drizzle-kit": "^0.31.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
